    "file",
    "explorer"
  ],
  "author": "thediggu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thediggu/ngelectron-explorer/issues"
  },
  "homepage": "https://github.com/thediggu/ngelectron-explorer#readme",
  "dependencies": {
    "angular": "^1.6.6",
    "electron": "^1.7.9",
    "fs.extra": "^1.3.2",
    "shelljs": "^0.7.8"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
